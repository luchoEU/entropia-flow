    "scripts": {
        "build:schema": "npx ts-json-schema-generator --path src/stream/data.ts --type StreamExportLayout --no-top-ref --expose all > src/stream/stream-export-layout.schema.json",
        "prebuild": "npm run build:schema",
        "predev": "npm run build:schema",
        "prewatch": "npm run build:schema",
        "prestream": "npm run build:schema",
        "prestream-dev": "npm run build:schema",
        "build": "webpack --config webpack.prod.js",
        "dev": "webpack --config webpack.dev.js",
        "watch": "webpack -w --config webpack.dev.js",
        "stream": "webpack --config webpack.stream.prod.js",
        "stream-dev": "webpack --config webpack.stream.dev.js",
        "stream-single": "webpack --config webpack.stream.singlefile.js",
        "test": "jest"
    },
