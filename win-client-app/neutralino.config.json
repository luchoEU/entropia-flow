{
  "applicationId": "com.entropia.flow",
  "defaultMode": "window",
  "port": 0,
  "url": "/resources/index.html",
  "enableServer": false,
  "logging": {
    "enabled": true
  },
  "window": {
    "title": "Entropia Flow",
    "width": 1024,
    "height": 720,
    "minWidth": 400,
    "minHeight": 300,
    "icon": "/resources/icon.png",
    "resizable": true,
    "exitProcessOnClose": true,
    "fullScreen": false,
    "alwaysOnTop": false,
    "enableInspector": true
  },
  "cli": {
    "binaryName": "entropia-flow",
    "resourcesPath": "/resources"
  }
}
